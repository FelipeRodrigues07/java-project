package aula;

public class Geladeira {

	private int canal;
	private String tamanho;
	private String modelo;
	
	Televisor(){
		canal = 4;
	    tamanho = "15 polegada";
	    modelo = "Brastenp";
	}
	
	Televisor (int c, String t, String m){
		canal = c;
		tamanho = t;
		modelo = m;
		
	}
	
	String getModelo() {
		return modelo;
	}
	
	
	String getTamanho()  {
		return tamanho;
	}
	
	
	

	public static void main(String[] args) {
		 Televisor tv1 = new Televisor();
		 Televisor tv2 = new Televisor(10, new String("10 polegadas"), new String("Gradiente"));
		 Televisor tv3 = new Televisor(10, new String("18 polegadas"), new String("Tramontina"));
		 
		 
		 System.out.println("O objeto tv1 é " + tv1.getModelo());
		 System.out.println("O objeto tv2 é " + tv2.getModelo());
		 System.out.println("O objeto tv3 é " + tv3.getTamanho());

	}
	
	
	
}
